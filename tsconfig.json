{
    "compilerOptions": {
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "strict": true,
      "target": "ES6",
      "module": "ESNext",
      "moduleResolution": "Node",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "~/*": ["./src/*"]
      },
      "lib": ["dom", "es2016"]
    },
    "include": ["./src/**/*"]
  }
  